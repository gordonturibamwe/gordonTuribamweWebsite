<p id="notice"><%= notice %></p>

<h1>Listing Pixes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Caption</th>
      <th>Home user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pixes.each do |pix| %>
      <tr>
        <td><%= pix.name %></td>
        <td><%= pix.caption %></td>
        <td><%= pix.home_user %></td>
        <td><%#= link_to 'Show', pix %></td>
        <td><%#= link_to 'Edit', edit_pix_path(pix) %></td>
        <td><%#= link_to 'Destroy', pix, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%#= link_to 'New Pix', new_pix_path %>
