<p id="notice"><%= notice %></p>

<h1>Listing Home Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @home_users.each do |home_user| %>
      <tr>
        <td><%= home_user.name %></td>
        <td><%= link_to 'Show', home_user %></td>
        <td><%= link_to 'Edit', edit_home_user_path(home_user) %></td>
        <td><%= link_to 'Destroy', home_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<hr>
<%= link_to 'Add Home Name', new_home_user_path, class: "btn btn-default btn-lg" %>
