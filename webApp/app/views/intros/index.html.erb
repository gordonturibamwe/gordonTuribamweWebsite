<p id="notice"><%= notice %></p>

<h1>Listing Intros</h1>

<table>
  <thead>
    <tr>
      <th>Intro</th>
      <th>Home-user</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intros.each do |intro| %>
      <tr>
        <td><%= intro.intro %></td>
        <td><%= intro.home_user %></td>
        <td><%= link_to 'Show', intro %></td>
        <td><%= link_to 'Edit', edit_intro_path(intro) %></td>
        <td><%= link_to 'Destroy', intro, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intro', new_home_user_intro_path(@home_user) %>
